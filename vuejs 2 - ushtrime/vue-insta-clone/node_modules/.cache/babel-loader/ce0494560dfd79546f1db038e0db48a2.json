,GADA,EACA;AAAA;;AACA,UAAA,KAAA,GAAA,GAAA,CAAA,MAAA,CAAA,KAAA;;AACA,UAAA,CAAA,KAAA,CAAA,MAAA,EAAA;AACA;AACA;;AACA,UAAA,MAAA,GAAA,IAAA,UAAA,EAAA;AACA,MAAA,MAAA,CAAA,aAAA,CAAA,KAAA,CAAA,CAAA,CAAA;;AACA,MAAA,MAAA,CAAA,MAAA,GAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,MAAA,CAAA,MAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,CAAA;AACA,OAHA;;AAKA,MAAA,QAAA,CAAA,aAAA,CAAA,OAAA,EAAA,KAAA,GAAA,EAAA;AACA,KAdA;AAeA,IAAA,QAfA,sBAeA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,cAAA,GAAA,EAAA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,WAAA,IAAA,GAAA,CAAA;AACA,KApBA;AAqBA,IAAA,SArBA,uBAqBA;AACA,UAAA,IAAA,GAAA;AACA,QAAA,QAAA,EAAA,aADA;AAEA,QAAA,SAAA,EAAA,oEAFA;AAGA,QAAA,SAAA,EAAA,KAAA,KAHA;AAIA,QAAA,KAAA,EAAA,CAJA;AAKA,QAAA,OAAA,EAAA,KAAA,OALA;AAMA,QAAA,MAAA,EAAA,KAAA;AANA,OAAA;AAQA,WAAA,KAAA,CAAA,OAAA,CAAA,IAAA;AACA,WAAA,QAAA;AACA;AAhCA,GAfA;AAiDA,EAAA,OAjDA,qBAiDA;AAAA;;AACA,IAAA,QAAA,CAAA,GAAA,CAAA,iBAAA,EAAA,UAAA,GAAA,EAAA;AACA,MAAA,MAAA,CAAA,cAAA,GAAA,GAAA,CAAA,MAAA;AACA,KAFA;AAGA;AArDA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"app-phone\">\n      <div class=\"phone-header\">\n        <img src=\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/1211695/vue_gram_logo_cp.png\" />\n        <a class=\"cancel-cta\" \n          v-if=\"step===2 || step===3\"\n          @click=\"goToHome\">Cancel\n          </a>\n          <a class=\"next-cta\" \n          v-if=\"step===2\"\n          @click=\"step++\">Next\n          </a>\n          <a class=\"next-cta\" \n          v-if=\"step===3\"\n          @click=\"sharePost\">Share\n          </a>\n      </div>\n      <phone-body \n        :posts=\"posts\"\n        :filters=\"filters\" \n        :step=\"step\"\n        :image=\"image\"\n        :selectedFilter=\"selectedFilter\"\n        v-model=\"caption\"/>\n      <div class=\"phone-footer\">\n       <div class=\"home-cta\" @click=\"goToHome\">\n        <i class=\"fas fa-home fa-lg\"></i>\n       </div>\n       <div class=\"upload-cta\">\n         <input type=\"file\"\n          name=\"file\"\n          id=\"file\"\n          class=\"inputfile\"\n          @change=\"uploadImage\">\n          <label for=\"file\">\n            <i class=\"far fa-plus-square fa-lg\" v-if=\"step === 1\"></i>\n          </label>\n       </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport PhoneBody from \"./components/PhoneBody\";\nimport posts from \"./data/posts\";\nimport filters from \"./data/filters\";\nimport EventBus from './event-bus.js'\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      posts,\n      filters,\n      step: 1,\n      image: \"\",\n      selectedFilter: \"\",\n      caption: \"\",\n    };\n  },\n  components: {\n    \"phone-body\": PhoneBody\n  },\n  methods: {\n    uploadImage(evt){\n      const files = evt.target.files;\n      if(!files.length){\n        return;\n      }\n      const reader = new FileReader();\n      reader.readAsDataURL(files[0]);\n      reader.onload = evt => {\n        this.image = evt.target.result;\n        this.step = 2;\n      }\n\n      document.querySelector(\"#file\").value = \"\";\n    },\n    goToHome(){\n      this.image = \"\"\n      this.selectedFilter = \"\"\n      this.caption = \"\"\n      this.step = 1\n    },\n    sharePost(){\n      const post = {\n        username: \"redon_emini\",\n        userImage: \"https://s3-us-west-2.amazonaws.com/s.cdpn.io/1211695/vue_lg_bg.png\",\n        postImage: this.image,\n        likes: 0,\n        caption: this.caption,\n        filter: this.filter\n      };\n      this.posts.unshift(post);\n      this.goToHome();\n    },\n  },\n  created(){\n    EventBus.$on('filter-selected', evt => {\n      this.selectedFilter = evt.filter\n    })\n  }\n};\n</script>\n\n<style lang=\"scss\" src=\"./styles/app.scss\">\n// Styles from stylesheet\n</style>"],"sourceRoot":"src"}]}