d(this.email, this.password)\n        .then(() => {\n          // Signed in \n          alert('Successfully registered')\n          this.$router.push('/')\n          // ...\n        })\n        .catch((error) => {\n          alert(error.message)\n          // ..\n        });\n    }\n  }\n}\n\n</script>\n\n"]}]}